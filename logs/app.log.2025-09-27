DEBUG 2025-09-27 13:29:30,942 utils 19016 12608 (0.000) INSERT INTO "news_newsdate" ("title", "image_url", "main_text", "author", "source", "views", "topic", "save_peaple_id", "created_at") VALUES ('В Пушкинском музее открылась выставка к 80-летию искусствоведа Марины Бессоновой', 'https://static.mk.ru/upload/entities/2025/09/23/16/articles/announcePic200/e7/6c/b2/7a/5ad5be3574605cdee51e79f55252135e.jpg', 'В Пушкинском музее, а точнее, в Галерее искусства стран Европы и Америки XIX–XX веков, открылась выставка, посвященная памяти Марины Бессоновой — знакового искусствоведа международного уровня. Память о Марине Александровне здесь берегут, а потому к ее 80-летию решили не просто почтить память своей легендарной коллеги, а рассказать о ней через призму выставки, которая бы открыла публике удивительное музейное «закулисье», связанное с личностью ученого, а также вспомнить о ее выставках и трудах.', '<a class="article-preview__author" href="https://www.mk.ru/authors/marinachechushkova/">Марина Чечушкова</a>', 'https://www.mk.ru/culture/', 0, 'culture', 'parser', '2025-09-27 10:29:30.941435') RETURNING "news_newsdate"."id"; args=('В Пушкинском музее открылась выставка к 80-летию искусствоведа Марины Бессоновой', 'https://static.mk.ru/upload/entities/2025/09/23/16/articles/announcePic200/e7/6c/b2/7a/5ad5be3574605cdee51e79f55252135e.jpg', 'В Пушкинском музее, а точнее, в Галерее искусства стран Европы и Америки XIX–XX веков, открылась выставка, посвященная памяти Марины Бессоновой — знакового искусствоведа международного уровня. Память о Марине Александровне здесь берегут, а потому к ее 80-летию решили не просто почтить память своей легендарной коллеги, а рассказать о ней через призму выставки, которая бы открыла публике удивительное музейное «закулисье», связанное с личностью ученого, а также вспомнить о ее выставках и трудах.', '<a class="article-preview__author" href="https://www.mk.ru/authors/marinachechushkova/">Марина Чечушкова</a>', 'https://www.mk.ru/culture/', 0,