DEBUG 2025-09-30 20:58:18,470 utils 17552 12008 (0.002) INSERT INTO "news_newsdate" ("title", "image_url", "main_text", "author", "source", "views", "topic", "save_peaple_id", "created_at") VALUES ('Пенсии в 2026 году проиндексируют только один раз, зато на процент выше годовой инфляции', 'https://static.mk.ru/upload/entities/2025/09/30/16/articles/announcePic200/88/9d/4b/e8/1ae6afa268b445927a39fc0c6487fe76.jpg', 'Правительство внесло в Государственную думу законопроект о бюджете Социального фонда на 2026-й и плановый период 2027-2028 годов. На выполнение социальных обязательств перед россиянами на предстоящую трехлетку в бюджете зарезервировано более 60 трлн рублей. Сама индексация страховых пенсий пройдет один раз – с 1 января, и выплаты увеличатся на 7,6%, примерно на один процент выше ожидаемой годовой инфляции. Ранее планировалось, что повышение пенсий в 2026 году будет осуществляться в два этапа.', '<a class="article-preview__author" href="https://www.mk.ru/authors/vladimir-chuprin/">Владимир Чуприн</a>', 'https://www.mk.ru/economics/', 4632, 'economics', 'parser', '2025-09-30 17:58:18.467973') RETURNING "news_newsdate"."id"; args=('Пенсии в 2026 году проиндексируют только один раз, зато на процент выше годовой инфляции', 'https://static.mk.ru/upload/entities/2025/09/30/16/articles/announcePic200/88/9d/4b/e8/1ae6afa268b445927a39fc0c6487fe76.jpg', 'Правительство внесло в Государственную думу законопроект о бюджете Социального фонда на 2026-й и плановый период 2027-2028 годов. На выполнение социальных обязательств перед россиянами на предстоящую трехлетку в бюджете зарезервировано более 60 трлн рублей. Сама индексация страховых пенсий пройдет один раз – с 1 января, и выплаты увеличатся на 7,6%, примерно на один процент выше ожидаемой годовой инфляции. Ранее планировалось, что повышение пенсий в 2026 году будет осуществляться в два этапа.', '<a class="article-preview__author" href="https://www.mk.ru/authors/vladimir-chuprin/">Владимир Чуприн</a>', 'https://www.mk